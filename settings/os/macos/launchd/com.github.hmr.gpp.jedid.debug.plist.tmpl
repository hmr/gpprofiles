<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>KeepAlive</key>
	<true/>
	<key>Label</key>
	<string>com.github.hmr.gpp.jedid</string>
	<key>ProgramArguments</key>
	<array>
		<string>__GPP_HOME__/libexec/tools/jedid</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/tmp/com.github.hmr.gpp/stderr.log</string>
	<key>StandardOutPath</key>
	<string>/tmp/com.github.hmr.gpp/stdout.log</string>
</dict>
</plist>
