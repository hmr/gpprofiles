{
  "title": "[GPP] General Puropose Profiles",
  "rules": [
    {
      "description": "[GPP] Single Left Command(⌘) to 英数 mode(for US layout)",
      "manipulators": [ {
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": { "optional": [ "any" ] }
          },
          "parameters": {
            "basic.to_if_held_down_threshold_milliseconds": 100
          },
          "to": [ {
              "key_code": "left_command",
              "lazy": true
            } ],
          "to_if_held_down": [ { "key_code": "left_command" } ],
          "to_if_alone": [ { "key_code": "japanese_eisuu" } ]
        } ]
    },

    {
      "description": "[GPP] Single Right Command(⌘) to かな mode(for US layout)",
      "manipulators": [ {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": { "optional": [ "any" ] }
          },
          "parameters": {
            "basic.to_if_held_down_threshold_milliseconds": 100
          },
          "to": [ {
              "key_code": "right_command",
              "lazy": true
            } ],
          "to_if_held_down": [ { "key_code": "right_command" } ],
          "to_if_alone": [ { "key_code": "japanese_kana" } ]
        } ]
    },

    {
      "description": "[GPP] Single Left Option(⌥) or Alt to 英数 mode(for US layout)",
      "manipulators": [ {
          "type": "basic",
          "from": {
            "key_code": "left_option",
            "modifiers": { "optional": [ "any" ] }
          },
          "parameters": {
            "basic.to_if_held_down_threshold_milliseconds": 100
          },
          "to": [ {
              "key_code": "left_option",
              "lazy": true
            } ],
          "to_if_held_down": [ { "key_code": "left_option" } ],
          "to_if_alone": [ { "key_code": "japanese_eisuu" } ]
        } ]
    },

    {
      "description": "[GPP] Single Right Option(⌥) or Alt to かな mode(for US layout)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_option",
            "modifiers": { "optional": [ "any" ] }
          },
          "parameters": { "basic.to_if_held_down_threshold_milliseconds": 100 },
          "to": [ {
              "key_code": "right_option",
              "lazy": true
            } ],
          "to_if_held_down": [ { "key_code": "right_option" } ],
          "to_if_alone": [ { "key_code": "japanese_kana" } ]
        } ]
    },

    {
      "description": "[GPP] Ctrl+K to search the web at browser",
      "manipulators": [ {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [ "control" ],
              "optional": ["any"] }
          },
          "to": [ {
              "key_code": "k",
              "modifiers": [ "left_command" ]
            } ],
          "conditions": [ {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^org\\.mozilla\\.firefox$",
                "^org\\.mozilla\\.nightly$",
                "^com\\.microsoft\\.Edge",
                "^com\\.google\\.Chrome$",
                "^com\\.apple\\.Safari$"
              ] }
          ] }
      ]
    },

    {
      "description": "[GPP] Send Option(Alt)+D when Command+D on iTerm2",
      "manipulators": [ {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": { "mandatory": [ "command" ] }
          },
          "to": [ {
              "key_code": "d",
              "modifiers": [ "left_option" ]
            } ],
          "conditions": [ {
            "type": "frontmost_application_if",
            "bundle_identifiers": [ "^com\\.googlecode\\.iterm2" ] } ]
        } ]
    },

    {
      "description": "[GPP] Ignore ⌘+R(reset terminal) on iTerm2",
      "manipulators": [ {
          "type": "basic",
          "from": {
            "key_code": "r",
            "modifiers": { "mandatory": [ "command" ] }
          },
          "conditions": [ {
            "type": "frontmost_application_if",
            "bundle_identifiers": [ "^com\\.googlecode\\.iterm2" ] } ]
        } ]
    },

    {
      "description": "[GPP] Ignore ⌘+K(clear buffer) on iTerm2",
      "manipulators": [ {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": { "mandatory": [ "command" ] }
          },
          "conditions": [ {
            "type": "frontmost_application_if",
            "bundle_identifiers": [ "^com\\.googlecode\\.iterm2" ] } ]
        } ]
    },

    {
      "description": "[GPP] Command+Option+C to open Calculator",
      "manipulators": [ {
          "type": "basic",
          "from": {
            "key_code": "g",
            "modifiers": {"mandatory": ["command", "option"] }
          },
          "to": [
            { "shell_command": "open -a 'Calculator'" }
          ]
        }
      ]
    },

